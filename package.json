{
  "name": "mealtime-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "npm run prisma:generate && next build --webpack",
    "start": "next start -H 0.0.0.0 -p $PORT || next start -H 0.0.0.0 -p 10000",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "prisma:seed": "npx prisma db seed",
    "db:reset": "npx prisma migrate reset --force",
    "db:clean": "npx ts-node --transpile-only prisma/clean-db.ts",
    "prisma:generate": "npx prisma generate",
    "prisma:clean": "if exist node_modules\\.prisma rmdir /s /q node_modules\\.prisma",
    "cron": "ts-node scripts/cron-runner.ts"
  },
